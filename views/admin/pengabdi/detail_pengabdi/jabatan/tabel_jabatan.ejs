<div class="tab-pane fade" id="show-jabatan" role="tabpanel" aria-labelledby="jabatan-tab">
  <!-- DataTales Example -->
  <div class="card shadow mb-4 mt-2">
    <div class="card-header py-3">
      <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#jabatanModal">
        <i class="fas fa-plus"></i>
        </button>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered tableData" id="jabatanTable" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>No</th>
              <th>Jabatan</th>
              <th>No SK <br> Tanggal SK</th>
              <th>TMT Jabatan</th>
              <th>Status</th>
              <th>Aksi</th>
            </tr>
          </thead>
          <tbody>
            <% for ( let i = 0; i < pengabdi.naikJabatanId.length; i++ ) { %>
              <tr>
                <td><%= i+1 %></td>
                <td> <%= pengabdi.naikJabatanId[i].jabatanId[0].jabatan %>  </td>
                <% var bulan = ['Januari', 'Februari', 'Maret','April','Mei','Juni','Juli','Agustus','September','Oktober','November','Desember']; %>
                <% var bulan2 = ['Januari', 'Februari', 'Maret','April','Mei','Juni','Juli','Agustus','September','Oktober','November','Desember']; %>
                <% var bulan3 = ['Januari', 'Februari', 'Maret','April','Mei','Juni','Juli','Agustus','September','Oktober','November','Desember']; %>
                <% var xbulan = pengabdi.naikJabatanId[i].tanggalSk.getMonth(); %>
                <% var bulan = bulan[xbulan]; %>
                <td>  <%= pengabdi.naikJabatanId[i].nomorSk%> <br> <%= `${pengabdi.naikJabatanId[i].tanggalSk.getDate()} ${bulan} ${pengabdi.naikJabatanId[i].tanggalSk.getFullYear()}` %> </td>
                <% var xbulan2 = pengabdi.naikJabatanId[i].tmtJabatanAwal.getMonth(); %>
                <% var bulan2 = bulan2[xbulan2]; %>
                <% var xbulan3 = pengabdi.naikJabatanId[i].tmtJabatanAkhir.getMonth(); %>
                <% var bulan3 = bulan3[xbulan3]; %>
                <td><%= `${pengabdi.naikJabatanId[i].tmtJabatanAwal.getDate()} ${bulan2} ${pengabdi.naikJabatanId[i].tmtJabatanAwal.getFullYear()}` %> - <%= `${pengabdi.naikJabatanId[i].tmtJabatanAkhir.getDate()} ${bulan3} ${pengabdi.naikJabatanId[i].tmtJabatanAkhir.getFullYear()}` %></td>
                <td class="text-center">
                <% if ( pengabdi.naikJabatanId[i].isAktif == false) { %>
                  <a href="javascript:void(0)" type="button" title="detail" class="btn btn-warning  btn-sm button-status"
                  data-idpengabdi="<%= pengabdi.id %>"
                  data-id="<%= pengabdi.naikJabatanId[i].id %>"
                  data-jabatan="<%= pengabdi.naikJabatanId[i].jabatanId[0].jabatan  %>"
                  ><i class="fas fa-cog"></i> Setup</a>
                  <% } else { %>
                    <button class="btn btn-success btn-sm"  disabled><i class="fas fa-check"></i> Aktif</button>
                  <% } %>
                </td>
                <td>
                  <a href="javascript:void(0)" type="button" title="detail" class="btn btn-primary btn-circle btn-sm button-detail"
                  data-foto ="<%= pengabdi.naikJabatanId[i].fotoUrl %>"
                  ><i class="fas fa-images"></i></a>
                  <a href="javascript:void(0)" type="button" title="edit" class="btn btn-warning btn-circle btn-sm button-update"
                  data-idpengabdi="<%= pengabdi.id %>"
                  data-id="<%= pengabdi.naikJabatanId[i].id %>"
                  data-jabatanid="<%= pengabdi.naikJabatanId[i].jabatanId[0].id %>"
                  data-nomorsk="<%= pengabdi.naikJabatanId[i].nomorSk %>"
                  data-tanggalsk="<% if ( pengabdi.naikJabatanId[i].tanggalSk) { %><%= pengabdi.naikJabatanId[i].tanggalSk.toISOString().split('T')[0] %><%}%>"
                  data-tmtjabatanawal="<% if ( pengabdi.naikJabatanId[i].tmtJabatanAwal) { %><%= pengabdi.naikJabatanId[i].tmtJabatanAwal.toISOString().split('T')[0] %><%}%>"
                  data-tmtjabatanakhir="<% if ( pengabdi.naikJabatanId[i].tmtJabatanAkhir) { %><%= pengabdi.naikJabatanId[i].tmtJabatanAkhir.toISOString().split('T')[0] %><%}%>"
                  ><i class="fas fa-edit"></i></a>
                    <button  title="hapus" class="btn btn-danger btn-circle btn-sm button-hapus" data-toggle="modal" data-target="#hapusModal"
                    data-idpengabdi="<%= pengabdi.id %>"
                    data-id="<%= pengabdi.naikJabatanId[i].id %>"
                    data-jabatan="<%= pengabdi.naikJabatanId[i].jabatanId[0].jabatan %>"
                    ><i class="fas fa-trash"></i></button>
                </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <!-- End of DataTables -->
</div>