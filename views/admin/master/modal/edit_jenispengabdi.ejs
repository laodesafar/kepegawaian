<!-- Modal -->
<div class="modal fade" id="editModal" tabindex="-1" aria-labelledby="editModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="editModalLabel">Edit Jenis Pengabdi</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <form action="/admin/jenispengabdi?_method=PUT" method="POST">
              <div class="modal-body">
                  <div class="form-group">
                    <label for="jenisPengabdi">Jenis Pengabdi</label>
                    <input type="text" class="form-control jenisPengabdi" name="jenisPengabdi" required>
                  </div>
                  <div class="invalid-feedback">
                    Jenis Pengabdi wajib diinputkan
                  </div>
                  <fieldset class="form-group">
                    <legend class="col-form-label">Bagian</legend>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input akademik" type="radio" name="bagian"   value="Akademik" required>
                        <label class="form-check-label" for="akademik">
                       Akademik
                        </label>
                    </div>
                    <div class="form-check form-check-inline">
                        <input class="form-check-input nonakademik" type="radio" name="bagian"   value="Non Akademik" required>
                        <label class="form-check-label" for="nonakademik">
                        Non Akademik
                        </label>
                    </div>
                </fieldset>
                <fieldset class="form-group">
                  <legend class="col-form-label">Jenis</legend>
                  <div class="form-check form-check-inline">
                      <input class="form-check-input true" type="radio" name="jenis" id="true"  value="true" required>
                      <label class="form-check-label" for="true">
                      Tetap
                      </label>
                  </div>
                  <div class="form-check form-check-inline">
                      <input class="form-check-input false" type="radio" name="jenis" id="false"  value="false" required>
                      <label class="form-check-label" for="false">
                      Tidak Tetap
                      </label>
                  </div>
              </fieldset>
              </div>
              <div class="modal-footer">
                  <input type="hidden" class="id" name="id">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Tutup</button>
                  <button type="submit" class="btn btn-primary">Update</button>
              </div>
          </form>
        </div>
      </div>
    </div>